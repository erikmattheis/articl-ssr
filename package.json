{
  "name": "monorepo",
  "version": "1.0.2",
  "private": true,
  "workspaces": [
    "backend",
    "frontend"
  ],
  "engines": {
    "node": ">=18.x"
  },
  "scripts": {
    "start": "start:backend",
    "start2": "npm-run-all --parallel start:backend start:frontend start:proxy",
