{
  "name": "monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "backend",
    "frontend"
  ],
  "scripts": {
    "start:backend": "npm run start --workspace backend",
    "start:frontend": "npm run dev --workspace frontend",
    "start:all": "npm-run-all --parallel start:backend start:frontend"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "vite": "^5.4.3"
  }
}
