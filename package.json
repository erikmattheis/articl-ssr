    "compression": "^1.7.4",
    "connect-history-api-fallback": "^2.0.0",
    "cors": "^2.8.5",
    "dayjs": "^1.11.8",
    "express": "^4.18.2",
    "http-proxy-middleware": "^2.0.6",
    "lodash": "^4.17.21",
    "path": "^0.12.7",
    "puppeteer": "^23.2.1",
    "serve-static": "^1.15.0",
    "url": "^0.11.1",
    "vue": "^3.5.1",
    "vue-cookies": "^1.8.3",
    "vue-draggable-next": "^2.2.1",
    "npm-run-all": "^4.1.5",
    "vue-feather": "^2.0.0",
    "vue-router": "^4.2.2",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
